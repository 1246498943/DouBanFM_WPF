<Window x:Class="SmokeMusic.Client.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SmokeMusic.Client"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Window.Background>
        <SolidColorBrush x:Name="BackgroundBrush" Color="{Binding UISettings.WindowBackground, Mode=OneWay}"></SolidColorBrush>
    </Window.Background>
    <Grid>
        <ListBox x:Name="PersonalChannels"   Style="{DynamicResource ChannelsStyle}" />
    </Grid>
</Window>
